{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "typeRoots": [
    "./node_modules/@types",
    "./node_modules/@yandex/ymaps3-types"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@i18n/*": [
        "src/i18n/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "ymaps3": [
        "./node_modules/@yandex/ymaps3-types"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}